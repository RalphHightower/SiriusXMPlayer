{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      },
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "%programdata%/SiriusXMPlayer/log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "SiriusXMPlayer"
    }
  },
  "PlayerSettings": {
    "SiriusXMStartupUrl": "https://player.siriusxm.com/favorites/channels"
  }
}